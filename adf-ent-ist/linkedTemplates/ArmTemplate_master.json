{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ent-ist"
		},
		"ls_sql_oltp_ist_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_oltp_ist'"
		},
		"ls_sql_olap_ist_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_olap_ist'"
		},
		"ls_akv_ent_ist_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-ent-ist.vault.azure.net/"
		},
		"ls_sa_ent_ist_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sec-key-ent-ist"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ent-ist_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sql_oltp_ist_connectionString": {
						"value": "[parameters('ls_sql_oltp_ist_connectionString')]"
					},
					"ls_sql_olap_ist_connectionString": {
						"value": "[parameters('ls_sql_olap_ist_connectionString')]"
					},
					"ls_akv_ent_ist_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_ent_ist_properties_typeProperties_baseUrl')]"
					},
					"ls_sa_ent_ist_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sa_ent_ist_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}